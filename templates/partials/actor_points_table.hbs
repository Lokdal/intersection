<div class="col_table">
    <div class="cell_header">Points</div>
    <div class="cell_data">Life</div>
    <div class="cell_data">Stamina</div>
    <div class="cell_data">Mana</div>
</div>
<div class="col_table">
    <div class="cell_header">Current</div>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.life_points.value}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.stamina_points.value}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.mana_points.value}}"/>
</div>
<div class="col_table">
    <div class="cell_header">Max</div>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.life_points.max}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.stamina_points.max}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.mana_points.max}}"/>
</div>

<div class="col_table"></div>

<div class="col_table">
    <div class="cell_header">Style</div>
    <div class="cell_data">Melee</div>
    <div class="cell_data">Ranged</div>
    <div class="cell_data">Magic</div>
</div>
<div class="col_table">
    <div class="cell_header">Offense</div>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.melee_off.total}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.ranged_off.total}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.magic_off.total}}"/>
</div>
<div class="col_table">
    <div class="cell_header">Defense</div>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.melee_def.total}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.ranged_def.total}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.magic_def.total}}"/>
</div>
<div class="col_table">
    <div class="cell_header">Level</div>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.melee_level}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.ranged_level}}"/>
    <input class="cell_data" type="number" data-dtype="Number" value="{{data.data.magic_level}}"/>
</div>